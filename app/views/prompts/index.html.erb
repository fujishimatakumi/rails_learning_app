<p id="notice"><%= notice %></p>

<h1>Prompts</h1>

<table>
  <thead>
    <tr>
      <th>General setting</th>
      <th>Text</th>
      <th>Translation text</th>
      <th>Prompt category</th>
      <th>Prompt tag</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @prompts.each do |prompt| %>
      <tr>
        <td><%= prompt.general_setting_id %></td>
        <td><%= prompt.text %></td>
        <td><%= prompt.translation_text %></td>
        <td><%= prompt.prompt_category_id %></td>
        <td><%= prompt.prompt_tag_id %></td>
        <td><%= link_to 'Show', prompt %></td>
        <td><%= link_to 'Edit', edit_prompt_path(prompt) %></td>
        <td><%= link_to 'Destroy', prompt, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Prompt', new_prompt_path %>
