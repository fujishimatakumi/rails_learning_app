<p id="notice"><%= notice %></p>

<h1>Check Points</h1>

<table>
  <thead>
    <tr>
      <th>General setting</th>
      <th>Name</th>
      <th>Discription</th>
      <th>Download url</th>
      <th>Extention category</th>
      <th>Extention tag</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @check_points.each do |check_point| %>
      <tr>
        <td><%= check_point.general_setting_id %></td>
        <td><%= check_point.name %></td>
        <td><%= check_point.discription %></td>
        <td><%= check_point.download_url %></td>
        <td><%= check_point.extention_category_id %></td>
        <td><%= check_point.extention_tag_id %></td>
        <td><%= link_to 'Show', check_point %></td>
        <td><%= link_to 'Edit', edit_check_point_path(check_point) %></td>
        <td><%= link_to 'Destroy', check_point, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Check Point', new_check_point_path %>
